mbs: ./bin/log.o ./bin/main.o ./bin/Module.o
	g++ -o ./mbs ./bin/main.o ./bin/log.o ./bin/Module.o /usr/local/lib/libyaml-cpp.a /usr/local/lib/libboost_filesystem.a /usr/local/lib/libboost_system.a

./bin/log.o: ./main-module/src/log.cpp
	g++ --std=c++11 -I./main-module/include -c ./main-module/src/log.cpp -o ./bin/log.o

./bin/main.o: ./main-module/src/main.cpp
	g++ --std=c++11 -I./main-module/include -c ./main-module/src/main.cpp -o ./bin/main.o

./bin/Module.o: ./main-module/src/Module.cpp
	g++ --std=c++11 -I./main-module/include -c ./main-module/src/Module.cpp -o ./bin/Module.o

clean:
	rm ./bin/*
	rm mbs
